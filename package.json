{
  "name": "stock-whisperer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": ["frontend"],
  "scripts": {
    "dev": "npm run dev --prefix frontend",
    "build": "npm run build --prefix frontend",
    "preview": "npm run preview --prefix frontend",
    "lint": "npm run lint --prefix frontend && eslint .",
    "install:all": "npm install && cd frontend && npm install",
    "postinstall": "npm install --prefix frontend",
    "smoke": "tsx scripts/smokeTest.ts",
    "invoke": "tsx scripts/testInvoke.ts",
    "vite:server": "tsx scripts/startViteServer.ts",
    "ff:detect": "tsx scripts/ffDetect.ts",
    "stai:reg": "tsx scripts/stai_regression.ts",
    "e2e:clean": "bash -lc 'lsof -ti :5173 -sTCP:LISTEN | xargs -r kill; lsof -ti :5174 -sTCP:LISTEN | xargs -r kill'",
    "e2e": "npm run -w frontend e2e",
    "e2e:dev": "npm run -w frontend e2e -- --project=chromium-dev",
    "e2e:preview": "npm run -w frontend e2e -- --project=chromium-preview",
    "e2e:grep": "npm run -w frontend e2e -- --grep"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.80.0",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.32.0",
    "globals": "^15.15.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.2.2"
  }
}
